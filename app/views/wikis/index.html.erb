<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<table id="wikis" class="display">
		  <thead>
			<tr>
			  <td>ID</td><td>Title</td><td>Edit</td>
			</tr>
		  </thead>
		  <tbody>
			<% @wikis.each do |wiki| %>
			<tr>
			  <td><%= wiki.user.id %></td><td><%= link_to wiki.title, wiki %></td><td><%= link_to "Edit", edit_wiki_path(wiki) %></td>
			</tr>
			<% end %>
		  </tbody>
		</table>
	</div>
</div>

<script type="text/javascript">
	$( document ).ready(function() {
		$('#wikis').dataTable({
			"sPaginationType": "bootstrap",
			"bJQueryUI": "true"
		});
	});
  
</script>